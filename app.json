{
  "expo": {
    "name": "Tsl Nigeria TV",
    "slug": "tsl-nigeria-tv",
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "com.tslnigeria.tsl-nigeria-tv",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "com.tslnigeria.tsl-nigeria-tv"
    },
    "android": {
      "package": "com.tslnigeria.tsl-nigeria-tv",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-google-signin/google-signin",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends and more..."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e06503cb-78c5-4156-a415-ff58d83d1db1"
      }
    }
  }
}
